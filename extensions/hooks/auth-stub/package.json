{
  "name": "@teselagen/directus-extension-auth-stub",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "main": "dist/index.js",
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^9.9.1"
  },
  "scripts": {
    "build": "directus-extension build",
    "watch-build": "directus-extension build --watch",
    "watch-index": "chokidar ./dist/*.js -c \"pnpm gen-index\"",
    "dev": "concurrently \"npm:watch-*\"",
    "gen-index-2": "printf '//%s' \"$(date)\"\n > index.js && cat index.temp.js >> index.js",
    "gen-index": "cat ./dist/index.js > index.js"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.9.1",
    "@types/node": "^17.0.27",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.1.0",
    "typescript": "^4.6.3"
  }
}
